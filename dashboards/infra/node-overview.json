    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus",
        "current": {
          "text": "Prometheus",
          "value": "Prometheus"
        }
      },
      {
        "name": "chain",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "$datasource"
        },
        "query": {
          "query": "label_values(up{chain!=\"\"}, chain)",
          "refId": "StandardVariableQuery"
        },
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        },
        "includeAll": true,
        "multi": true,
        "allValue": ".+"
      },
      {
        "name": "instance",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "$datasource"
        },
        "query": {
          "query": "label_values(up{chain=~\"$chain\"}, instance)",
          "refId": "StandardVariableQuery"
        },
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        },
        "includeAll": true,
        "multi": true,
        "allValue": ".*"
      },

    {
      "id": 20,
      "title": "Solana Validators",
      "type": "table",
      "gridPos": {
        "h": 4,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "up{chain=\"solana\",instance=~\"$instance\",instance!~\"localhost.*\"}",
          "format": "table",
          "instant": true,
          "refId": "A"
        },
        {
          "expr": "solana_node_healthy{chain=\"solana\",instance=~\"$instance\"}",
          "format": "table",
          "instant": true,
          "refId": "B"
        },
        {
          "expr": "solana_node_synced{chain=\"solana\",instance=~\"$instance\"}",
          "format": "table",
          "instant": true,
          "refId": "C"
        },
        {
          "expr": "label_replace(solana_node_version_info{chain=\"solana\",instance=~\"$instance\"}, \"node_version\", \"$1\", \"version\", \"(.*)\")",
          "format": "table",
          "instant": true,
          "refId": "D"
        },
        {
          "expr": "label_replace(solana_latest_version_info{chain=\"solana\",instance=~\"$instance\"}, \"latest_version\", \"$1\", \"version\", \"(.*)\")",
          "format": "table",
          "instant": true,
          "refId": "E"
        },
        {
          "expr": "solana_network_peers{chain=\"solana\",instance=~\"$instance\"}",
          "format": "table",
          "instant": true,
          "refId": "F"
        },
        {
          "expr": "solana_node_block_height{chain=\"solana\",instance=~\"$instance\"}",
          "format": "table",
          "instant": true,
          "refId": "G"
        },
        {
          "expr": "solana_network_block_height{chain=\"solana\",instance=~\"$instance\"}",
          "format": "table",
          "instant": true,
          "refId": "H"
        },
        {
          "expr": "solana_node_epoch{chain=\"solana\",instance=~\"$instance\"}",
          "format": "table",
          "instant": true,
          "refId": "I"
        },
        {
          "expr": "solana_validator_activated_stake_lamports{chain=\"solana\",instance=~\"$instance\"} / 1e9",
          "format": "table",
          "instant": true,
          "refId": "J"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto",
            "inspect": false
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Time" },
            "properties": [{ "id": "custom.hidden", "value": true }]
          },
          {
            "matcher": { "id": "byName", "options": "Status" },
            "properties": [
              { "id": "custom.cellOptions", "value": { "type": "color-background", "mode": "basic" } },
              { "id": "mappings", "value": [{ "type": "value", "options": { "0": { "text": "Down", "color": "red" }, "1": { "text": "Up", "color": "green" } } }] }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Healthy" },
            "properties": [
              { "id": "custom.cellOptions", "value": { "type": "color-background", "mode": "basic" } },
              { "id": "mappings", "value": [{ "type": "value", "options": { "0": { "text": "Unhealthy", "color": "red" }, "1": { "text": "Healthy", "color": "green" } } }] }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Synced" },
            "properties": [
              { "id": "custom.cellOptions", "value": { "type": "color-background", "mode": "basic" } },
              { "id": "mappings", "value": [{ "type": "value", "options": { "0": { "text": "Syncing", "color": "yellow" }, "1": { "text": "Synced", "color": "green" } } }] }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Block Height" },
            "properties": [
              { "id": "unit", "value": "locale" },
              { "id": "custom.cellOptions", "value": { "type": "color-text" } },
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "blue" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Network Height" },
            "properties": [
              { "id": "unit", "value": "locale" },
              { "id": "custom.cellOptions", "value": { "type": "color-text" } },
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "light-blue" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Epoch" },
            "properties": [
              { "id": "unit", "value": "locale" },
              { "id": "custom.cellOptions", "value": { "type": "color-text" } },
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "purple" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Stake (SOL)" },
            "properties": [
              { "id": "unit", "value": "locale" },
              { "id": "decimals", "value": 0 },
              { "id": "custom.cellOptions", "value": { "type": "color-text" } },
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "orange" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Latest Version" },
            "properties": [
              { "id": "custom.cellOptions", "value": { "type": "color-text" } },
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "light-green" } }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "desc": false, "displayName": "instance" }],
        "cellHeight": "sm",
        "footer": { "show": false }
      },
      "transformations": [
        { "id": "merge", "options": {} },
        {
          "id": "groupBy",
          "options": {
            "fields": {
              "instance": { "aggregations": [], "operation": "groupby" },
              "Value #A": { "aggregations": ["lastNotNull"], "operation": "aggregate" },
              "Value #B": { "aggregations": ["lastNotNull"], "operation": "aggregate" },
              "Value #C": { "aggregations": ["lastNotNull"], "operation": "aggregate" },
              "node_version": { "aggregations": ["lastNotNull"], "operation": "aggregate" },
              "latest_version": { "aggregations": ["lastNotNull"], "operation": "aggregate" },
              "Value #F": { "aggregations": ["lastNotNull"], "operation": "aggregate" },
              "Value #G": { "aggregations": ["lastNotNull"], "operation": "aggregate" },
              "Value #H": { "aggregations": ["lastNotNull"], "operation": "aggregate" },
              "Value #I": { "aggregations": ["lastNotNull"], "operation": "aggregate" },
              "Value #J": { "aggregations": ["lastNotNull"], "operation": "aggregate" }
            }
          }
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true, "Value #D (lastNotNull)": true, "Value #E (lastNotNull)": true },
            "indexByName": {
              "instance": 0,
              "Value #A (lastNotNull)": 1,
              "Value #B (lastNotNull)": 2,
              "Value #C (lastNotNull)": 3,
              "node_version (lastNotNull)": 4,
              "latest_version (lastNotNull)": 5,
              "Value #F (lastNotNull)": 6,
              "Value #G (lastNotNull)": 7,
              "Value #H (lastNotNull)": 8,
              "Value #I (lastNotNull)": 9,
              "Value #J (lastNotNull)": 10
            },
            "renameByName": {
              "Value #A (lastNotNull)": "Status",
              "Value #B (lastNotNull)": "Healthy",
              "Value #C (lastNotNull)": "Synced",
              "node_version (lastNotNull)": "Version",
              "latest_version (lastNotNull)": "Latest Version",
              "Value #F (lastNotNull)": "Peers",
              "Value #G (lastNotNull)": "Block Height",
              "Value #H (lastNotNull)": "Network Height",
              "Value #I (lastNotNull)": "Epoch",
              "Value #J (lastNotNull)": "Stake (SOL)"
            }
          }
        }
      ]
    },

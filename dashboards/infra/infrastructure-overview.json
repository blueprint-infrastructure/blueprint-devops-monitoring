{
  "title": "Infrastructure Overview",
  "tags": ["infrastructure", "blockchain", "nodes"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "title": "All Nodes",
      "type": "table",
      "gridPos": {
        "h": 12,
        "w": 18,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "targets": [
        {
          "expr": "up{instance=~\"$instance\"}",
          "format": "table",
          "instant": true,
          "refId": "healthy"
        },
        {
          "expr": "time() - node_boot_time_seconds{instance=~\"$instance\"}",
          "format": "table",
          "instant": true,
          "refId": "uptime"
        },
        {
          "expr": "count by (instance) (node_cpu_seconds_total{mode=\"idle\",instance=~\"$instance\"})",
          "format": "table",
          "instant": true,
          "refId": "cpu_cores"
        },
        {
          "expr": "node_memory_MemTotal_bytes{instance=~\"$instance\"}",
          "format": "table",
          "instant": true,
          "refId": "mem_total"
        },
        {
          "expr": "100 - (avg by (instance) (rate(node_cpu_seconds_total{mode=\"idle\",instance=~\"$instance\"}[5m])) * 100)",
          "format": "table",
          "instant": true,
          "refId": "cpu_pct"
        },
        {
          "expr": "100 - ((node_memory_MemAvailable_bytes{instance=~\"$instance\"} / node_memory_MemTotal_bytes{instance=~\"$instance\"}) * 100)",
          "format": "table",
          "instant": true,
          "refId": "mem_pct"
        },
        {
          "expr": "100 - ((node_filesystem_avail_bytes{mountpoint=\"/\",fstype!=\"tmpfs\",instance=~\"$instance\"} / node_filesystem_size_bytes{mountpoint=\"/\",fstype!=\"tmpfs\",instance=~\"$instance\"}) * 100)",
          "format": "table",
          "instant": true,
          "refId": "disk_pct"
        },
        {
          "expr": "sum by (instance) (rate(node_disk_read_bytes_total{instance=~\"$instance\"}[5m]))",
          "format": "table",
          "instant": true,
          "refId": "disk_read"
        },
        {
          "expr": "sum by (instance) (rate(node_disk_written_bytes_total{instance=~\"$instance\"}[5m]))",
          "format": "table",
          "instant": true,
          "refId": "disk_write"
        },
        {
          "expr": "sum by (instance) (rate(node_network_receive_bytes_total{device!~\"lo|docker.*|veth.*|br.*\",instance=~\"$instance\"}[5m]))",
          "format": "table",
          "instant": true,
          "refId": "net_rx"
        },
        {
          "expr": "sum by (instance) (rate(node_network_transmit_bytes_total{device!~\"lo|docker.*|veth.*|br.*\",instance=~\"$instance\"}[5m]))",
          "format": "table",
          "instant": true,
          "refId": "net_tx"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left",
            "displayMode": "auto",
            "inspect": false,
            "filterable": true
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Time" },
            "properties": [{ "id": "custom.hidden", "value": true }]
          },
          {
            "matcher": { "id": "byName", "options": "instance" },
            "properties": [
              { "id": "displayName", "value": "Host" },
              { "id": "custom.width", "value": 260 },
              { "id": "links", "value": [{ "title": "Drill down", "url": "/d/node-detail?var-instance=${__value.text}" }] }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Value #healthy" },
            "properties": [
              { "id": "displayName", "value": "Healthy" },
              { "id": "custom.width", "value": 80 },
              { "id": "custom.displayMode", "value": "color-background-solid" },
              { "id": "mappings", "value": [{ "type": "value", "options": { "0": { "text": "Down", "color": "red" }, "1": { "text": "Up", "color": "green" } } }] },
              { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "value": null, "color": "red" }, { "value": 1, "color": "green" }] } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Value #uptime" },
            "properties": [
              { "id": "displayName", "value": "Uptime" },
              { "id": "custom.width", "value": 90 },
              { "id": "unit", "value": "dtdurations" }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Value #cpu_cores" },
            "properties": [
              { "id": "displayName", "value": "CPU" },
              { "id": "custom.width", "value": 50 },
              { "id": "unit", "value": "short" },
              { "id": "decimals", "value": 1 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Value #mem_total" },
            "properties": [
              { "id": "displayName", "value": "Mem" },
              { "id": "custom.width", "value": 70 },
              { "id": "unit", "value": "bytes" },
              { "id": "decimals", "value": 1 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Value #cpu_pct" },
            "properties": [
              { "id": "displayName", "value": "CPU %" },
              { "id": "custom.width", "value": 70 },
              { "id": "unit", "value": "percent" },
              { "id": "decimals", "value": 1 },
              { "id": "custom.displayMode", "value": "color-background-solid" },
              { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "value": null, "color": "green" }, { "value": 70, "color": "yellow" }, { "value": 85, "color": "red" }] } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Value #mem_pct" },
            "properties": [
              { "id": "displayName", "value": "Mem %" },
              { "id": "custom.width", "value": 70 },
              { "id": "unit", "value": "percent" },
              { "id": "decimals", "value": 1 },
              { "id": "custom.displayMode", "value": "color-background-solid" },
              { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "value": null, "color": "green" }, { "value": 70, "color": "yellow" }, { "value": 85, "color": "red" }] } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Value #disk_pct" },
            "properties": [
              { "id": "displayName", "value": "Disk %" },
              { "id": "custom.width", "value": 70 },
              { "id": "unit", "value": "percent" },
              { "id": "decimals", "value": 1 },
              { "id": "custom.displayMode", "value": "color-background-solid" },
              { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "value": null, "color": "green" }, { "value": 80, "color": "yellow" }, { "value": 90, "color": "red" }] } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Value #disk_read" },
            "properties": [
              { "id": "displayName", "value": "Disk read" },
              { "id": "custom.width", "value": 85 },
              { "id": "unit", "value": "Bps" },
              { "id": "decimals", "value": 1 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Value #disk_write" },
            "properties": [
              { "id": "displayName", "value": "Disk write" },
              { "id": "custom.width", "value": 85 },
              { "id": "unit", "value": "Bps" },
              { "id": "decimals", "value": 1 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Value #net_rx" },
            "properties": [
              { "id": "displayName", "value": "Receive" },
              { "id": "custom.width", "value": 85 },
              { "id": "unit", "value": "Bps" },
              { "id": "decimals", "value": 1 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Value #net_tx" },
            "properties": [
              { "id": "displayName", "value": "Transmit" },
              { "id": "custom.width", "value": 85 },
              { "id": "unit", "value": "Bps" },
              { "id": "decimals", "value": 1 }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "cellHeight": "sm",
        "sortBy": [{ "desc": false, "displayName": "Host" }]
      },
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "__name__": true,
              "job": true,
              "mountpoint": true,
              "fstype": true,
              "device": true,
              "mode": true
            },
            "indexByName": {
              "instance": 0,
              "Value #healthy": 1,
              "Value #uptime": 2,
              "Value #cpu_cores": 3,
              "Value #mem_total": 4,
              "Value #cpu_pct": 5,
              "Value #mem_pct": 6,
              "Value #disk_pct": 7,
              "Value #disk_read": 8,
              "Value #disk_write": 9,
              "Value #net_rx": 10,
              "Value #net_tx": 11
            }
          }
        }
      ]
    },
    {
